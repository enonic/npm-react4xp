{ // https://www.typescriptlang.org/tsconfig
	"compilerOptions": {
		"allowJs": true,
		"baseUrl": ".",
		"declaration": false,
		"esModuleInterop": true,
		"jsx": "react-jsx",
		"lib": [
			"ES2020",
		],

		"module": "ES2020",

		"moduleResolution": "node",
		"noEmit": true,
		"resolveJsonModule": true,
		"rootDir": "..",
		"skipLibCheck": true,
		"strictNullChecks": false,
		"target": "ES2020",
	},
	"exclude": [],
	"include": [
		"./**/*.ts"
	]
}
